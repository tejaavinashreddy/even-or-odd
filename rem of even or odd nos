import java.util.Scanner;
public class OddandEven{

 public static void main(String[] args) {
    Scanner cs=new Scanner(System.in);
    int num;
    System.out.println("Enter the number:");
    num=cs.nextInt();
    int odd=0,even=0;
    int rem=0;
    while(num!=0)
    {
        rem=num%10;
        if(rem%2==0)
    
       System.out.print(rem);
    
       num=num/10;
      
    }
  
    
}
 
 }
